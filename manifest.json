{
  "name": "Codecov Extension",
  "version": "0.1.0",
  "homepage_url": "https://github.com/codecov/chrome-plugin",
  "description": "Coverage reports overlay in Github.",
  "offline_enabled": false,
  "manifest_version": 2,
  "icons": {
    "16": "icons/codecov-16.png",
    "48": "icons/codecov-48.png",
    "128": "icons/codecov-128.png"
  },
  "options_ui": {
    "page": "dist/options.html",
    "chrome_style": true
  },
  "content_scripts": [
    {
      "matches": ["*://github.com/*"],
      "all_frames": true,
      "run_at": "document_end",
      "js": ["dist/jquery-2.1.3.min.js", "dist/github.js"]
    }
  ],
  "web_accessible_resources": [
    "dist/jquery-2.1.3.min.js",
    "dist/github.js",
    "dist/github.css"
  ],
  "permissions": [
    "*://github.com/*/blob/*",
    "*://github.com/*/pull/*",
    "*://github.com/*/compare/*",
    "*://github.com/*/commit/*",
    "*://github.com/*/blame/*",
    "*://codecov.io/*",
    "storage"
  ]
}
